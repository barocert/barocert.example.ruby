<div id="content">
  <p class="heading1">Barocert SDK Ruby On Rails Example</p>
  <br/>
  <fieldset class="fieldset1">
    <legend><%= request.env['PATH_INFO'] %></legend>
    <ul>
      <li>ReceiptID (접수아이디) : <%= @Response['receiptID'] %></li>
      <li>State (상태) : <%= @Response['state'] %></li>
      <% @Response["multiSignedData"].each_with_index do |signedData, index| %>
        <fieldset class="fieldset2">
          <ul>
            <li>SignedData (전자서명 데이터 전문) : <%= signedData %></li>
          </ul>
        </fieldset>
      <% end %>
      <li>Ci (연계정보) : <%= @Response['ci'] %></li>
    </ul>
  </fieldset>
</div>
